<Window x:Class="WareMaster.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        
        xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="InventoryInit" Content="InventoryInit" HorizontalAlignment="Left" Height="34" Margin="90,117,0,0" VerticalAlignment="Top" Width="137" Click="InventoryInit_Click"/>
        <Button Name="items" Content="Go to items" Height="50" Width="100" Click="items_Click"></Button>
        <Button x:Name="InventoryInbound" Content="InventoryInbound" HorizontalAlignment="Left" Height="34" Margin="90,175,0,0" VerticalAlignment="Top" Width="137" Click="InventoryInbound_Click"/>
        <Button x:Name="InventoryOutbound" Content="InventoryOutbound" HorizontalAlignment="Left" Height="34" Margin="90,231,0,0" VerticalAlignment="Top" Width="137" Click="InventoryOutbound_Click"/>

    </Grid>
</Window>
